  "license": "GPL-3.0-or-later",
  "dependencies": {
    "@allmaps/annotation": "^1.0.0-beta.12",
    "@allmaps/iiif-parser": "^1.0.0-beta.23",
    "@allmaps/transform": "^1.0.0-beta.13",
    "commander": "^10.0.0",
    "stream-json": "^1.7.4",
    "svg-parser": "^2.0.4"
  },
  "devDependencies": {
    "@types/node": "^18.14.2",
    "@types/stream-json": "^1.7.2",
    "@types/svg-parser": "^2.0.3",
    "chai": "^4.3.6",
    "eslint": "^8.35.0",
    "mocha": "^8.4.0",
    "typescript": "^4.9.5",
    "zod": "^3.20.6"
  }
