{
  "name": "@allmaps/latest",
  "version": "1.0.0-beta.33",
  "author": {
    "name": "Bert Spaan",
    "email": "hello@bertspaan.nl",
    "url": "https://bertspaan.nl"
  },
  "description": "The latest maps georeferenced with Allmaps Editor",
  "svelte": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/allmaps/allmaps.git",
    "directory": "apps/latest"
  },
  "license": "GPL-3.0-or-later",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "vite dev",
    "watch": "svelte-package --watch",
    "build": "vite build && pnpm run package",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "pnpm run package",
    "lerna-build": "lerna run build --scope @allmaps/latest --include-dependencies",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check src && eslint src --ext .ts,.svelte",
    "format": "prettier --write src",
    "types": "tsc --noEmit",
    "preversion": "pnpm run types && pnpm run lint"
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "peerDependencies": {
    "svelte": "^4.0.0"
  },
  "dependencies": {
    "@allmaps/annotation": "^1.0.0-beta.27",
    "@allmaps/id": "^1.0.0-beta.27",
    "@allmaps/tailwind": "^1.0.0-beta.12",
    "@allmaps/transform": "^1.0.0-beta.38",
    "@allmaps/types": "^1.0.0-beta.13",
    "@allmaps/ui": "^1.0.0-beta.29",
    "@turf/rewind": "^6.5.0",
    "d3-geo": "^3.0.1"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/package": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@tailwindcss/container-queries": "^0.1.1",
    "@types/d3-geo": "^3.0.2",
    "@types/eslint": "^8.56.0",
    "@types/geojson": "^7946.0.10",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "postcss": "^8.4.18",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "publint": "^0.1.9",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.0",
    "vite": "^5.0.3"
  }
}
