{
  "name": "@allmaps/latest",
  "version": "1.0.0-beta.43",
  "author": {
    "name": "Bert Spaan",
    "email": "hello@bertspaan.nl",
    "url": "https://bertspaan.nl"
  },
  "description": "The latest maps georeferenced with Allmaps Editor",
  "svelte": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/allmaps/allmaps.git",
    "directory": "apps/latest"
  },
  "license": "GPL-3.0-or-later",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "vite dev",
    "watch": "svelte-package --watch",
    "build": "vite build && pnpm run package",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "pnpm run package",
    "lerna-build": "lerna run build --scope @allmaps/latest --include-dependencies",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write src",
    "types": "tsc --noEmit"
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "peerDependencies": {
    "svelte": "^4.0.0"
  },
  "dependencies": {
    "@allmaps/annotation": "workspace:^",
    "@allmaps/id": "workspace:^",
    "@allmaps/project": "workspace:^",
    "@allmaps/stdlib": "workspace:^",
    "@allmaps/tailwind": "workspace:^",
    "@allmaps/types": "workspace:^",
    "@allmaps/ui": "workspace:^",
    "@turf/rewind": "^7.2.0",
    "d3-geo": "^3.0.1"
  },
  "devDependencies": {
    "@allmaps/types": "workspace:^",
    "@eslint/compat": "^1.2.5",
    "@eslint/js": "^9.18.0",
    "@sveltejs/adapter-static": "^3.0.0",
    "@sveltejs/kit": "^2.21.5",
    "@sveltejs/package": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@tailwindcss/vite": "^4.0.0",
    "@turf/length": "^7.2.0",
    "@types/d3-geo": "^3.0.2",
    "@types/geojson": "^7946.0.10",
    "@types/lodash-es": "^4.17.12",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vitejs/plugin-basic-ssl": "^1.2.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-svelte": "^3.0.0",
    "globals": "^16.0.0",
    "prettier": "^3.6.2",
    "publint": "^0.3.2",
    "prettier-plugin-svelte": "^3.4.0",
    "svelte": "^5.34.3",
    "svelte-check": "^4.0.0",
    "tailwindcss": "^4.1.0",
    "typescript": "^5.7.0",
    "typescript-eslint": "^8.20.0",
    "vite": "^6.2.6",
    "vite-plugin-devtools-json": "^1.0.0"
  }
}
