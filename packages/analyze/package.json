    "@allmaps/types": "workspace:^",
    "point-in-polygon-hao": "^1.2.4"
  },
  "devDependencies": {
    "@types/eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "documentation": "^14.0.0",
    "eslint": "^8.56.0",
