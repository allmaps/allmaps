  "author": {
    "name": "Manuel Claeys Bouuaert",
    "email": "manuel.claeys.b@gmail.com",
    "url": "https://manuelclaeysbouuaert.be"
  },
  "description": "Allmaps Analysis Library",
  "type": "module",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
