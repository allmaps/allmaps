    ".": {
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/allmaps/allmaps.git",
    "directory": "packages/analyze"
  },
  "homepage": "https://allmaps.org",
  "keywords": [
    "allmaps",
