{
  "name": "@allmaps/iiif-parser",
  "version": "1.0.0-beta.29",
  "author": {
    "name": "Bert Spaan",
    "email": "hello@bertspaan.nl",
    "url": "https://bertspaan.nl"
  },
  "description": "Allmaps IIIF parser",
  "type": "module",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/allmaps/allmaps.git",
    "directory": "packages/iiif-parser"
  },
  "homepage": "https://allmaps.org",
  "keywords": [
    "allmaps",
    "iiif",
    "parser"
  ],
  "scripts": {
    "watch": "tsc --watch",
    "build": "tsc && npm run documentation",
    "preversion": "npm run types && npm run lint && npm test",
    "types": "tsc --noEmit",
    "test": "NODE_ENV=test mocha",
    "pretest": "npm run build",
    "lint": "prettier --check src test && eslint src test --ext .js,.ts",
    "documentation": "documentation readme dist/index.js --markdown-toc-max-depth=3 --section=API",
    "format": "prettier --write src test"
  },
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "@types/node": "^18.14.2",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "chai": "^4.3.6",
    "chai-shallow-deep-equal": "^1.4.6",
    "documentation": "^14.0.0",
    "eslint": "^8.35.0",
    "mocha": "^8.4.0",
    "prettier": "^2.8.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@allmaps/types": "^1.0.0-beta.7",
    "zod": "^3.20.6"
  }
}
