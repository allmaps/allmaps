{
  "name": "@allmaps/render",
  "version": "1.0.0-beta.63",
  "contributors": [
    {
      "name": "Bert Spaan",
      "email": "hello@bertspaan.nl",
      "url": "https://bertspaan.nl"
    },
    {
      "name": "Manuel Claeys Bouuaert",
      "email": "manuel.claeys.b@gmail.com",
      "url": "https://manuelclaeysbouuaert.be"
    }
  ],
  "description": "Render functions for WebGL and image buffers",
  "type": "module",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/bundled/index.umd.js",
      "import": "./dist/index.js"
    },
    "./int-array": {
      "types": "./dist/int-array.d.ts",
      "import": "./dist/int-array.js"
    },
    "./canvas": {
      "types": "./dist/canvas.d.ts",
      "import": "./dist/canvas.js"
    },
    "./webgl2": {
      "types": "./dist/webgl2.d.ts",
      "import": "./dist/webgl2.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/allmaps/allmaps.git",
    "directory": "packages/render"
  },
  "scripts": {
    "dev": "vite",
    "watch": "vite build --watch",
    "build": "vite build",
    "preview": "vite preview",
    "pretest": "pnpm run build",
    "test": "NODE_ENV=test mocha",
    "preversion": "pnpm run types && pnpm run lint && pnpm test",
    "lint": "prettier --check src test && eslint src test --ext .js,.ts",
    "types": "tsc --noEmit",
    "format": "prettier --write src test",
    "documentation": "documentation readme dist/index.js --markdown-toc-max-depth=3 --section=API"
  },
  "homepage": "https://allmaps.org",
  "keywords": [
    "allmaps",
    "georeference",
    "map"
  ],
  "dependencies": {
    "@allmaps/annotation": "^1.0.0-beta.27",
    "@allmaps/id": "^1.0.0-beta.27",
    "@allmaps/iiif-parser": "^1.0.0-beta.37",
    "@allmaps/stdlib": "^1.0.0-beta.28",
    "@allmaps/transform": "^1.0.0-beta.38",
    "@allmaps/triangulate": "^1.0.0-beta.20",
    "@allmaps/types": "^1.0.0-beta.13",
    "@turf/boolean-point-in-polygon": "^6.5.0",
    "lodash-es": "^4.17.21",
    "potpack": "^2.0.0",
    "rbush": "^3.0.1",
    "robust-point-in-polygon": "^1.0.3"
  },
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "@types/eslint": "^8.56.0",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.14.2",
    "@types/rbush": "^3.0.0",
    "@types/robust-point-in-polygon": "^1.0.4",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "chai": "^4.3.6",
    "documentation": "^14.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "mocha": "^8.4.0",
    "prettier": "^2.8.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.3",
    "vite-plugin-dts": "^3.8.3",
    "vite-plugin-glsl": "^1.3.0",
    "vite-plugin-no-bundle": "^4.0.0"
  },
  "gitHead": "f372db912469ba4415c94f79ea2806c3866ff987"
}
